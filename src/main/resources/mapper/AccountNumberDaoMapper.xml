<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.dao.AccountNumberDao">
    <resultMap id="AccountNumberMap" type="com.imooc.entity.AccountNumber">
        <id column="account_number_id" property="accountNumberId"></id>
        <result column="username" property="username"></result>
        <result column="password" property="password"></result>
        <result column="create_time" property="createTime"></result>
        <result column="end_login_time" property="endLoginTime"></result>
        <result column="islocked" property="islocked"></result>

    </resultMap>

    <select id="selectPasspord" resultMap="AccountNumberMap">
        select * from account_number
    </select>

    <select id="selectByAccountId" resultMap="AccountNumberMap">
        select * from account_number where account_number_id = #{account_number_id}
    </select>
</mapper>